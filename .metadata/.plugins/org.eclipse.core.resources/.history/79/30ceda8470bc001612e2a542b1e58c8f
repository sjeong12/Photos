<script language=javascript>
function valid_check()
{
	if (document.frm_d.name.value == "") 
	{
		alert("세부 카테고리를 선택하세요");
		document.frm_d.name.focus(); 
		return false;
	}

	if (document.frm_d.category.value == "") 
	{
		alert("카테고리를 선택하세요");
		document.frm_d.category.focus(); 
		return false;
	}
	
	document.frm_file.submit();
}


function submit_list()
{
	document.frm_file.action = "/LSJ_PHOTO/home.jsp";
	document.frm_file.submit();
}

 </script>
<h3>디자인 생성</h3>
<BODY>
<FORM NAME = "frm_d" ACTION = "admin/addProduct_ok.jsp" METHOD = "post" ENCTYPE="multipart/form-data">
<TABLE WIDTH = "600" BORDER = "0" CellPadding = "0" CellSpacing = "0">

	<TR>
		<INPUT TYPE = "hidden" NAME = pcode VALUE = "code">
	</TR>
	<TR>
		<TD WIDTH = "40%" ALIGN = "left">디자인 이름</TD>
		<TD WIDTH = "60%" ALIGN = "left">
			<INPUT TYPE ="text" NAME="name" SIZE = "50" MAXLENGTH = "50">
		</TD>
	</TR>
	<TR>
		<TD WIDTH = "40%" ALIGN = "left">카테고리</TD>
		<TD WIDTH = "60%" ALIGN = "left">
			<INPUT TYPE ="text" NAME="category" SIZE = "50" MAXLENGTH = "50">
		</TD>
	</TR>
	<TR>
		<TD WIDTH = "100%" ALIGN = "center" COLSPAN = "2">
		<TABLE>
			<TR>
				<TD WIDTH = "34%" ALIGN = "center">
					<INPUT TYPE = "button" VALUE = "등록" onClick="valid_check()">
				</TD>
			</TR>
		</TABLE>
		</TD>
	</TR>
</TABLE>
</FORM>
</BODY>
